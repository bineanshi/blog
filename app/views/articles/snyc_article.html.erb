<%= link_to 'sites', sites_path %>

<%= form_tag(get_article_articles_path, class:  "valid_engine mt10") do %>
  <%= hidden_field_tag "back",params[:back] || articles_path%>
  <%= select_tag 'site_rule_id',options_for_select(SiteRule.all.map{|sr| ["#{sr.site_name}--#{sr.rule_name}",sr.id]})%>
  <%= text_field_tag "k"%>
  <%= submit_tag "导入","data-loading-text" => "处理中..." %>
<% end %>