<h1>Listing articles</h1>
<div class="table-responsive">
<table class="table table-bordered">
  <thead>
    <tr>
      <th>Title</th>
      <th>Tabloid</th>
      <th>Author name</th>
      <!-- <th>Content</th> -->
<!--       <th>Submited at</th>
      <th>Is publish</th>
      <th>Desc</th>
      <th>Article category</th> -->
      <th>Source category name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @articles.each do |article| %>
      <tr>
        <td><%= link_to_blank article.title , article.article_url %></td>
        <td><%= article.tabloid %></td>
        <td><%= article.author_name %></td>
<!--         <td><%#= article.content.html_safe %></td>
        <td><%#= article.submited_at %></td>
        <td><%#= article.is_publish %></td>
        <td><%#= article.desc %></td> -->
<!--         <td><%#= article.article_category_id %></td> -->
        <td><%= article.source_category_name %></td>
        <td><%= link_to 'Show', article %></td>
        <td><%= link_to 'Edit', edit_article_path(article) %></td>
        <td><%= link_to 'Destroy', article, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
<br>

<%= link_to 'New Article', snyc_article_articles_path %>
